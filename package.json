{
  "name": "elide-simple-rest",
  "description": "Elide simple REST data provider for refine.",
  "version": "0.0.3",
  "license": "MIT",
  "author": "Chirdeep Tomar",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "./refine.config.js"
  ],
  "scripts": {
    "attw": "attw --pack .",
    "build": "tsup && node ./generate-declarations.js",
    "dev": "tsup --watch",
    "prepare": "pnpm build",
    "publint": "publint --strict=true --level=suggestion",
    "test": "jest --passWithNoTests --runInBand",
    "types": "node ./generate-declarations.js"
  },
  "dependencies": {
    "axios": "1.7.9",
    "query-string": "9.1.1"
  },
  "devDependencies": {
    "@esbuild-plugins/node-resolve": "0.2.2",
    "@refinedev/cli": "^2.16.42",
    "@refinedev/core": "^4.57.1",
    "@types/jest": "^29.2.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "nock": "^13.4.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "tsup": "8.3.5",
    "typescript": "^5.7.2",
    "warn-once": "0.1.1"
  },
  "peerDependencies": {
    "@refinedev/core": "^4.46.1"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  }
}