    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "tsup": "^5.11.13"
  },
  "dependencies": {
    "@pankod/refine-core": "^3.18.0",
    "axios": "^0.27.2",
    "query-string": "^7.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}